{
  "go.formatTool": "gofmt",
  "go.buildOnSave": "package",
  "go.lintOnSave": "package",
  "go.vetOnSave": "package",
  "go.testOnSave": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": true,
  "gitlens.hovers.currentLine.enabled": true,
  "gitlens.keymap": "chorded",
  "window.restoreWindows": "all",
  "window.openFilesInNewWindow": "default",
  "editor.formatOnSave": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.sideBar.location": "left",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.renderWhitespace": "none",
  "editor.minimap.enabled": false,
  "git.confirmSync": false,
  "files.exclude": {
    "**/*.pyc": true
  },
  "terminal.integrated.shell.osx": "zsh",
  "terminal.integrated.shell.linux": "zsh",
  "terminal.integrated.lineHeight": 1.2,
  "workbench.panel.defaultLocation": "right",
  "terminal.integrated.rendererType": "dom",
  "workbench.colorTheme": "Twilight",
  "python.showStartPage": false,
  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "python.pythonPath": "${env:PYTHON}",
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "vim.vimrc.enable": true,
  "vim.useSystemClipboard": true,
  "vim.leader": ";",
  "vim.hlsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["K"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": [":noh"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["editor.action.toggleRenderWhitespace"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.moveEditorLeftInGroup"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.moveEditorRightInGroup"]
    },
    {
      "before": ["<leader>", "w"],
      "after": ["v", "i", "w"]
    },
    {
      "before": ["<leader>", "W"],
      "after": ["v", "i", "W"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "shellcheck.run": "onSave",
  "rust.build_on_save": true,
  "rust.clippy_preference": "on",
  "debug.allowBreakpointsEverywhere": true,
  "go.useLanguageServer": true,
  "vs-kubernetes": {
    "vs-kubernetes.helm-path.mac": "${env:HOME}/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.minikube-path.mac": "${env:HOME}/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.draft-path.mac": "${env:HOME}/.vs-kubernetes/tools/draft/darwin-amd64/draft"
  },
  "jupyter.alwaysTrustNotebooks": true,
  "latex-workshop.message.update.show": false,
  "go.toolsManagement.autoUpdate": true,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "editor.lineNumbers": "on",
  "workbench.editor.untitled.hint": "hidden",
  "[elm]": {
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  }
}
